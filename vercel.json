{


    "version" : 2,
    "builds" : [
      {
        "src" : "src/index.js",
        "use" : "@vercel/node"
      }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "https://studynotion-edtech-project-1.onrender.com",
            "methods" : ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers" : {
              "Access-Control-Allow-Origin" : "*"
            }
        }
    ]
}
